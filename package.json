{
  "name": "BE",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "build": "nest build",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "prepare": "node -e \"if(process.env.HUSKY==='0' || process.env.CI) process.exit(0);\" && husky",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:build": "docker build --platform linux/amd64 -t choihooo/bakmyunjun-be:latest --target production .",
    "docker:push": "docker push choihooo/bakmyunjun-be:latest",
    "docker:build:push": "pnpm docker:build && pnpm docker:push",
    "db:check": "ts-node -r tsconfig-paths/register scripts/check-db-connection.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "cp .env.development .env && prisma migrate dev",
    "prisma:migrate:prod": "cp .env.production .env && prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "cp .env.development .env && prisma studio",
    "prisma:studio:prod": "cp .env.production .env && prisma studio",
    "prisma:format": "prisma format",
    "prisma:seed": "cp .env.development .env && ts-node prisma/seed.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postman:generate": "ts-node -r tsconfig-paths/register scripts/generate-postman-collection.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/client": "^5.22.0",
    "prisma": "^5.22.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^8.1.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-kakao": "^1.0.3",
    "@types/pino-http": "^6.1.0",
    "@types/supertest": "^6.0.2",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "openapi-to-postmanv2": "^5.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
